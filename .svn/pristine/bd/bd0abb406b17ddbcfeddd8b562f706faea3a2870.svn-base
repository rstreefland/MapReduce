package uk.co.streefland.rhys.mapreduce.mapreduce;

/**
 * Represents a Key-Value pair used by mapreduce
 */
public class Pair<K, V> {

    private final K key;
    private final V value;

    public Pair(K key, V value) {
        this.key = key;
        this.value = value;
    }

    public K getKey() {
        return key;
    }

    public V getValue() {
        return value;
    }

}
